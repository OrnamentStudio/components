{
  "name": "ornament-components",
  "version": "0.0.2",
  "description": "React Components",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "preview": "babel preview/src --out-dir preview/lib --copy-files",
    "start": "npm run build -- --watch & npm run preview -- --watch & gulp & node ./preview/server.js",
    "test": "npm run build && mocha \"test/**/*.js\"",
    "lint": "eslint --ext .es,.js --ignore-path .eslintignore --format codeframe ./ && stylint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrnamentStudio/components.git"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "Abylay Keldibek <atomio.ak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OrnamentStudio/components/issues"
  },
  "homepage": "https://github.com/OrnamentStudio/components#readme",
  "engines": {
    "node": "^8.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "browserify": "16.2.2",
    "chai": "^4.1.2",
    "classnames": "^2.2.6",
    "connect-livereload": "0.6.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "4.16.3",
    "gulp": "^4.0.0",
    "livereload": "0.7.0",
    "marked": "^0.4.0",
    "mocha": "^5.2.0",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router": "^4.3.1",
    "react-side-effect": "^1.1.5",
    "stylint": "^1.5.9",
    "stylint-stylish": "^1.4.1"
  },
  "peerDependencies": {
    "classnames": "^2.x.x",
    "marked": "^0.x.x",
    "react": "^16.x.x",
    "react-router": "^4.x.x",
    "react-side-effect": "^1.x.x",
    "prop-types": "^15.x.x"
  }
}
